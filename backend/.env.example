# Database (Supabase or Local PostgreSQL)
# For Supabase: Use the connection string from your Supabase project settings
# DATABASE_URL="postgresql://postgres.[PROJECT-REF]:[PASSWORD]@aws-0-us-east-1.pooler.supabase.com:5432/postgres"
# For Local:
DATABASE_URL="postgresql://postgres:password@localhost:5432/aads_db?schema=public"

# Supabase (Optional - for additional Supabase features)
SUPABASE_URL="https://your-project.supabase.co"
SUPABASE_ANON_KEY="your-anon-key"

# Server
PORT=3001
NODE_ENV=development

# JWT
JWT_SECRET=your-secret-key-change-this-in-production
JWT_EXPIRES_IN=7d

# CORS
CORS_ORIGIN=http://localhost:5173
